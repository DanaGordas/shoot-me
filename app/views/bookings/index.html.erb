<div class="container p-10"><h2>My bookings</h2></div>
<div class="container"><% @bookings.each do |b| %>
  <div class="card-product">
    <!--hardcoded image-->
    <img src="https://images.unsplash.com/photo-1493246318656-5bfd4cfb29b8?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1350&q=80" />
    <div class="card-product-infos">
      <h2><%= link_to b.location.name, bookings_path(@booking) %></h2>
      <p>from: <strong><%= b.start_date %> </strong> to <strong><%= b.end_date %></strong></p>
      </div>
      <!-- #status should be here -->
    </div>
    <br>
  <% end %>
</div>



